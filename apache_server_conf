Listen 8080

<VirtualHost *:8080>
LoadModule php5_module /opt/circleci/php/7.0.7/libexec/apache2/libphp5.so


   DocumentRoot /home/ubuntu/PHPLaravelSpike/apache_server_conf/public
   ServerName host.example.com
   <FilesMatch \.php$>
   SetHandler application/x-httpd-php
  </FilesMatch>
</VirtualHost>
